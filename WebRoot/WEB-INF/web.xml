<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  
  <servlet>
    <servlet-name>SearchResult</servlet-name>
    <servlet-class>com.news.servlet.SearchResult</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchResult</servlet-name>
    <url-pattern>/SearchResult</url-pattern>
  </servlet-mapping> 
  <servlet>
  	<servlet-name>NewsResult</servlet-name>
  	<servlet-class>com.news.servlet.NewsResult</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>NewsResult</servlet-name>
  	<url-pattern>/NewsResult</url-pattern>
  </servlet-mapping>

  <servlet>
  	<servlet-name>RecentNews</servlet-name>
  	<servlet-class>com.news.servlet.RecentNews</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RecentNews</servlet-name>
  	<url-pattern>/RecentNews</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>RecentNews</welcome-file>
  </welcome-file-list>

  <servlet>
  	<servlet-name>SimiNews</servlet-name>
  	<servlet-class>com.news.servlet.SimiNews</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SimiNews</servlet-name>
  	<url-pattern>/SimiNews</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>RecentNews</welcome-file>
  </welcome-file-list>
</web-app>